{
  "schemaVersion": 1,
  "id": "minepkg-companion",
  "version": "${version}",
  "description": "A companion mod for minepkg. It would be lonely without it.",
  "contact": {
    "email": "info@minepkg.io",
    "homepage": "https://minepkg.io/",
    "issues": "https://github.com/minepkg/fabric-companion/issues",
    "sources": "https://github.com/minepkg/fabric-companion.git"
  },
  "authors": [
    "Fourmisain",
    "jasonhaxstuff"
  ],
  "license": "MIT",
  "icon": "assets/minepkg-companion/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "io.minepkg.companion.common.MinepkgCompanion"
    ],
    "preLaunch": [
      "io.minepkg.companion.common.GluePreLaunch"
    ]
  },
  "mixins": [
    "minepkg-companion.common.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.12.0"
  },
  "custom": {
    "modmenu": {
      "badges": [
        "library"
      ]
    }
  }
}
